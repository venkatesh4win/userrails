<!DOCTYPE html>
<html>
  <head>
    <title>User</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <% if admin_signed_in? %>

  <ul>
  	<li><%= link_to 'submit link', new_link_path %></li>
  	<li><%= link_to 'Account', edit_admin_registration_path %></li>
  	<li><%= link_to 'sign out', destroy_admin_session_path, :method =>:delete %></li>
  </ul>
  <% else %>
 <ul>
 	<li><%= link_to 'sign up', new_admin_registration_path %></li>
  	<li><%= link_to 'sign in', new_admin_session_path %></li>
 </ul>
 <% end %>
    <%= yield %>
  </body>
</html>
